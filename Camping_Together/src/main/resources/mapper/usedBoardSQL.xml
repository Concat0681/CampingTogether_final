<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="usedBoard">
	<insert id="insertUesdBoard" parameterType="ub">
		insert into used_board 
		values(used_board_seq.nextval, #{usedBoardWriter}, #{usedBoardTitle}, #{usedBoardContent}, #{usedBoardCategory}, 0, #{usedProductStatus}, #{exchangeStatus}, 0, #{usedTradeLocation}, 0, to_char(sysdate, 'yyyy-mm-dd'))
	</insert>
</mapper>
